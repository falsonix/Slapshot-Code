ex;\n\n// User defined function\nvoid myblockfunction_spooky_scary_skeletons();\n// User defined function\nvoid myblockfunction_rest();\n\nint Brain_precision = 0;\n\nfloat myVariable;\n\n// User defined function\nvoid myblockfunction_spooky_scary_skeletons() {\n  Brain.playNote(3, 5, 250);\n  Brain.playNote(3, 5, 250);\n  Brain.playNote(3, 4, 250);\n  Brain.playNote(3, 4, 250);\n  Brain.playNote(3, 2, 250);\n  Brain.playNote(3, 3, 250);\n  Brain.playNote(3, 2, 250);\n  Brain.playNote(3, 2, 250);\n  Brain.playNote(3, 5, 250);\n  Brain.playNote(3, 5, 250);\n  Brain.playNote(3, 4, 250);\n  Brain.playNote(3, 4, 250);\n  Brain.playNote(3, 2, 250);\n  myblockfunction_rest();\n  Brain.playNote(3, 5, 250);\n  Brain.playNote(3, 5, 250);\n  Brain.playNote(3, 4, 250);\n  Brain.playNote(3, 4, 250);\n  Brain.playNote(3, 2, 250);\n  Brain.playNote(3, 3, 250);\n  Brain.playNote(3, 2, 250);\n  Brain.playNote(3, 3, 250);\n  Brain.playNote(3, 4, 250);\n  Brain.playNote(3, 2, 250);\n  Brain.playNote(3, 3, 250);\n  Brain.playNote(3, 2, 250);\n  Brain.playNote(3, 1, 250);\n}\n\n// User defined function\nvoid myblockfunction_rest() {\n  // Rests for a quarter note\n  wait(1.0, seconds);\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  // Code written by Christian Greene. Feel free to use and modify.\n  // Please note that VEX IQ brains can only play quarter notes and not eighth notes.\n  Brain.Screen.clearScreen();\n  Brain.Screen.setCursor(1, 1);\n  // Enter custom text messages here if you want to\n  Brain.Screen.print(\"Enjoy the music!\");\n  Brain.Screen.newLine();\n  Brain.Screen.print(\"Press Up to restart\");\n  // Set a delay\n  wait(1.0, seconds);\n  while (true) {\n    // Play your song of choice. I used Spooky Scary Skeletons in this as a demonstration\n    myblockfunction_spooky_scary_skeletons();\n    waitUntil(Brain.buttonUp.pressing());\n  wait(20, msec);\n  }\n  return 0;\n}\n\n\nint main() {\n  whenStarted1();\n}","target":"Physical"}